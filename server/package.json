{
  "name": "server",
  "homepage": ".",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "build:ts": "tsc --project ./",
    "debug:ts": "node --inspect --require ts-node/register src/server.ts",
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*' -e ts,tsx,js  --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "start:debug": "node server.js",
    "build": "NODE_ENV=production tsc",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "license": "Internal",
  "main": "lib/server.js",
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/ramda": "^0.27.40",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.3",
    "date-fns": "^2.19.0",
    "dom-parser": "^0.1.6",
    "dotenv": "^8.2.0",
    "express-async-router": "^0.1.15",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "node-fetch": "^2.6.1",
    "node-html-parser": "^3.3.5",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.8",
    "ramda": "^0.27.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "firebase-functions-test": "^0.2.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "puppeteer": "^10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.3"
  },
  "private": true
}
